{
  "expo": {
    "name": "MuscleMemory",
    "slug": "MuscleMemory",
    "version": "1.0.0",
    "orientation": "portrait",
    "description": "Dead simple workout tracker app.",
    "icon": "./assets/images/icon.png",
    "scheme": "musclememory",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.logmoon.musclememory",
      "versionCode": 1
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 150,
          "resizeMode": "contain",
          "backgroundColor": "#121212"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7ff1f864-b9f7-4004-9c5f-88cfeb1dab1b"
      }
    }
  }
}
